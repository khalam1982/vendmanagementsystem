  <types>
    
  
  	<!-- schema for device command objects -->
    <schema elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
      targetNamespace="urn:command.ws.unity.trilliantnetworks.com">

      <import namespace="urn:ws.unity.trilliantnetworks.com" />
      <import namespace="urn:util.command.ws.unity.trilliantnetworks.com" />
      <import namespace="urn:event.command.ws.unity.trilliantnetworks.com" />
      
      <complexType name="deviceCommand" abstract="true">
        <sequence>
          <element name="description" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
          <element name="devicePk" type="xsd:long" nillable="true" minOccurs="0" maxOccurs="1" />
          <element name="deviceId" type="xsd:string" nillable="true" minOccurs="1" maxOccurs="1" />
          <element name="deviceType" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
          <element name="functionId" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
          <element name="deviceStatus" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
          <element name="runId" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
        </sequence>
      </complexType>

      <complexType name="compositeDeviceCommand" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="deviceCommands" nillable ="false" type="dcns:deviceCommand" minOccurs="0" maxOccurs="unbounded" />
      			</sequence>
      		</extension>
      	</complexContent>
      </complexType>

      <complexType name="nd08Command" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="zone" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateTemperatureUnit" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateHeatingSetpoint" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateCoolingSetpoint" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateHvacMode" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateFanMode" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateOccupiedMode" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="temperatureUnit" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="heatingSetpoint" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="coolingSetpoint" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="hvacMode" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="fanMode" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="occupiedMode" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="indoorTemperature" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="outdoorTemperature" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="heatingStatus" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="coolingStatus" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="fanStatus" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />  
					<element name="forceTransfer" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />   
					<element name="updateDuplicate" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />     				       				   				
      			</sequence>
      		</extension>
      	</complexContent>
      </complexType>

      <complexType name="nd04Command" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="action" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      			</sequence>
      		</extension>
      	</complexContent>
      </complexType>
      
      <complexType name="nd05Command" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="maxDownloadCount" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="downloadTimeoutInSeconds" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="ignoreGaps" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="includeOnlyAptitles" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      			</sequence>
      		</extension>
      	</complexContent>
      </complexType>
      <complexType name="genericAdHocCommand" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="partnerId" type="xsd:string" nillable="false" minOccurs="0" maxOccurs="1" />
      				<element name="adHocType" type="xsd:string" nillable="false" minOccurs="0" maxOccurs="1" />
      				<element name="userId" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="deviceParameter" type="dcuns:DeviceParameter" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="specificInformation" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      			</sequence>
      		</extension>
      	</complexContent>
      </complexType>
      <complexType name="nd09Command" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="relay" type="dcuns:Relay" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="numberOfRelays" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="forceTransfer" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateDuplicate" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="loadSheddingEntries" type="dcuns:ND09LoadSheddingEntry" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="loadSheddingParams" type="dcuns:ND09LoadSheddingEntry" nillable="true" minOccurs="0" maxOccurs="1" />
      			</sequence>
      		</extension>
      	</complexContent>
      </complexType>
      
      <complexType name="nd12Command" >
    	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="state" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      			</sequence>
      		</extension>
      	</complexContent>
    </complexType>
    
    <complexType name="eg01Command" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="area" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="billingPoint" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="drProgram" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="feederIdentifier" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="forceTransfer" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
					<element name="eventParams" type="dcens:EG01Event" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="loadSheddingEvents" type="dcens:LoadSheddingEvent" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="priceEvents" type="dcens:PriceEvent" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="eg01ThermostatHoldControlEvent" type="dcens:EG01ThermostatHoldControlEvent" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="eg01ThermostatModeControlEvent" type="dcens:EG01ThermostatModeControlEvent" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="sourceUtility" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="thermostatStatus" type="dcuns:ThermostatStatus" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateDuplicate" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="messageEvent" type="dcens:MessageEvent" nillable="true" minOccurs="0" maxOccurs="1" />
      			</sequence>
      		</extension>
      	</complexContent>
	</complexType>
	
	<complexType name="az01Command"> 
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="seasonID" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="month" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="day" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="dayIdArray" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="unbounded" />      				
					<element name="yearArray" type="dcuns:AZ01Year" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="dayArray" type="dcuns:AZ01Day" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="rateArray" type="dcuns:AZ01Rate" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="costArray" type="dcuns:AZ01Cost" nillable="true" minOccurs="0" maxOccurs="unbounded" />
      				<element name="prepayConfig" type="dcuns:AZ01PrepayConfig" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="prepayAccount" type="dcuns:AZ01PrepayAccount" nillable="true" minOccurs="0" maxOccurs="1" />
      			</sequence>
      		</extension>
      	</complexContent>
	</complexType>
	
	<complexType name="nd10Command" >
      	<complexContent>
      		<extension base="dcns:deviceCommand">
      			<sequence>
      				<element name="forceTransfer" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
      				<element name="updateDuplicate" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
      			</sequence>
      		</extension>
      	</complexContent>
     </complexType>
     
     <complexType name="deviceParameterCommand" >
     	<complexContent>
     		<extension base="dcns:deviceCommand">
     		 	<sequence>
     				<element name="deviceParameterType" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
     				<element name="deviceParameter" type="dcuns:DeviceParameter" nillable="true" minOccurs="0" maxOccurs="1" />
     			</sequence>
     		</extension>
     	</complexContent>
     </complexType>
     
     <complexType name="lg01BillingPeriodConfigCommand" >
     	<complexContent>
     		<extension base="dcns:deviceCommand">
     		 	<sequence>
     				<element name="startMonth" type="xsd:short" nillable="true" minOccurs="0" maxOccurs="1" />
     				<element name="startDate" type="xsd:short" nillable="true" minOccurs="0" maxOccurs="1" />
     				<element name="days" type="xsd:short" nillable="true" minOccurs="0" maxOccurs="1" />
     				<element name="months" type="xsd:short" nillable="true" minOccurs="0" maxOccurs="1" />
     			</sequence>
     		</extension>
     	</complexContent>
     </complexType>    
    
     <complexType name="lg01BlockTariffThresholdsConfigCommand" >
     	<complexContent>
     		<extension base="dcns:deviceCommand">
     		 	<sequence>
     				<element name="blockTariffThresholds" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="unbounded" />
     			</sequence>
     		</extension>
     	</complexContent>
     </complexType>
     
     <complexType name="lg01CreditModeCommand" >
     	<complexContent>
     		<extension base="dcns:deviceCommand">
     		 	<sequence>
     				<element name="creditMode" type="dcuns:LG01CreditMode" nillable="true" minOccurs="0" maxOccurs="1" />
     			</sequence>
     		</extension>
     	</complexContent>
     </complexType>
     
     <complexType name="lg01IntervalDataConfigCommand" >
     	<complexContent>
     		<extension base="dcns:deviceCommand">
     		 	<sequence>
     				<element name="utcStartTime" type="xsd:dateTime" nillable="true" minOccurs="0" maxOccurs="1" />
     				<element name="utcEndTime" type="xsd:dateTime" nillable="true" minOccurs="0" maxOccurs="1" />
     			</sequence>
     		</extension>
     	</complexContent>
     </complexType>
     
     <complexType name="lg01ScheduledReadsConfigCommand" >
     	<complexContent>
     		<extension base="dcns:deviceCommand">
     		 	<sequence>
     				<element name="period" type="dcuns:LG01Period" nillable="true" minOccurs="0" maxOccurs="1" />
     				<element name="randomizeReportTransmission" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
     			</sequence>
     		</extension>
     	</complexContent>
     </complexType>
     
     <complexType name="Lg01TariffPricesConfigCommand" >
     	<complexContent>
     		<extension base="dcns:deviceCommand">
     		 	<sequence>
     				<element name="tariffPrices" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="unbounded" />
     				<element name="futureRateActivationDate" type="xsd:dateTime" nillable="true" minOccurs="0" maxOccurs="1" />
     			</sequence>
     		</extension>
     	</complexContent>
     </complexType>
    </schema>
    
    <!-- COMMAND UTIL SCHEMA -->
    <schema elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema"
      targetNamespace="urn:util.command.ws.unity.trilliantnetworks.com">  		

      <import namespace="urn:ws.unity.trilliantnetworks.com" />
      <import namespace="urn:tobject.ws.unity.trilliantnetworks.com" />
      <import namespace="urn:command.ws.unity.trilliantnetworks.com" />
      
      <complexType name="Relay">
      		<sequence>
		      	<element name="state" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1"/>
		      	<element name="label" type="xsd:string" nillable="true" minOccurs="0" maxOccurs="1" />
     				<element name="controlledState" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />	
     				<element name="coldLoadPickup" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />	      	
	      	</sequence>
      </complexType>

      <complexType name="ThermostatStatus">
            <sequence>
	          <element name="communicationLinkDown" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="configurationError" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="controlMode" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="coolSetpoint" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="day" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="daysRemaining" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="equipmentType" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="eventCancelledByUser" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="eventDisabledByUser" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="fanMode" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="fanOperation" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="filterFault" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="heatPumpDisabled" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="heatPumpFault" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="heatSetpoint" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="holdSetpointNumber" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="holdType" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="hoursRemaining" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="loadSheddingInProgress" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="lowBattery" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="mandatoryLoadShedding" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="month" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="nbrOfCoolingStages" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="nbrOfHeatingStages" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="nbrOfStagesEngaged" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="newMessage" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="operationMode" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="outputShortCircuit" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="priceEventInProgress" type="xsd:boolean" default="false" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="temperature" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="temperatureScaleDisplayed" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />	          
            </sequence>
      </complexType>

      <complexType name="ND09LoadSheddingEntry">
            <sequence>
	          <element name="cyclingCtrl" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="duration" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="randomEnd" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="randomStart" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="startDateTime" type="xsd:dateTime" nillable="true" minOccurs="0" maxOccurs="1" />
            </sequence>
      </complexType>
      
      <complexType name="AZ01Year">
            <sequence>
	          <element name="dayOfYear" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="dayID" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
            </sequence>
      </complexType>

      <complexType name="AZ01Day">
            <sequence>
	          <element name="dayID" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="rateIDArray" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="unbounded" />
            </sequence>
      </complexType>

      <complexType name="AZ01Cost">
            <sequence>
	          <element name="costID" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="type" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="cost" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
            </sequence>
      </complexType>

      <complexType name="AZ01Rate">
            <sequence>
	          <element name="rateID" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="pattern" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="color" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="symbolSelector" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="symbolCount" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="typeLimited" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <!-- typeContinuous was spelled incorrectly as typeContinious in unityclient so I have to match it for the mapper to succeed -->
	          <element name="typeContinious" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" /> 
	          <element name="acknowledge" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="tone" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="advanceWarningHours" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="cost" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="limit" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
	          <element name="next" type="xsd:int" nillable="true" minOccurs="0" maxOccurs="1" />
            </sequence>
      </complexType>

      <complexType name="AZ01PrepayAccount">
            <sequence>
              <element name="prepayEnabled" type="xsd:boolean" nillable="true" minOccurs="0" maxOccurs="1" />
