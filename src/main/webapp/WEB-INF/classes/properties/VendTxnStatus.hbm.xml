<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 
<hibernate-mapping>
<class name="com.centrica.vms.model.VendTxnStatus" table="VEND_STATUS_HISTORY">
	<id column="VEND_STATUS_ID" name="vendStatusTxnsHisID" type="java.lang.Integer">
		<generator class="sequence">
		<param name="sequence">SEQ_VMS_VENDTXNSTATUS</param>
		</generator>
	</id>
	<property column="STATUS" name="status"  type="java.lang.Integer" />
	<property column="UPDATED_TIMESTAMP" name="updatedTimeStamp" type="java.util.Date"/>
