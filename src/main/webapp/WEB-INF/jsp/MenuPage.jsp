<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd"><%@taglib uri="http://struts.apache.org/tags-bean" prefix="bean"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%><%@ taglib prefix="logic" uri="http://struts.apache.org/tags-logic" %><html><head><script type="text/javascript">function logOff(){		var answer = confirm ("Log Off?");	if(answer){		document.forms[0].action = 'VMSHome';		document.forms[0].target = '_parent';		document.forms[0].dispatch.value = 'logOut';		document.forms[0].submit();	}	}function submitPage(menu){	if(menu == '00'){		document.forms[0].action = 'HomePage';		document.forms[0].target = '_parent';		document.forms[0].dispatch.value = '';	}else if(menu == '01'){		document.forms[0].action = 'VendRequestAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'requestPage';	}else if(menu == '02'){		document.forms[0].action = 'AdjustmentRequestAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'requestPage';	}else if(menu == '03'){		document.forms[0].action = 'VendHistorySearchAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'searchPage';	}	else if(menu == '04'){		document.forms[0].action = 'MeterDispatchSearchAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'searchPage';	}	else if(menu == '05'){		document.forms[0].action = 'UtilityRequestAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'requestTypePage';	}	else if(menu == '06'){		document.forms[0].action = 'PowerUserAdminAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'groupPage';	}	else if(menu == '07'){		document.forms[0].action = 'UserAdminAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'searchPage';			}	else if(menu == '08'){		document.forms[0].action = 'ChangePwdAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'passwordPage';			}	else if(menu == '09') {		document.forms[0].action = 'VendReportAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'reportPage';	} 	else if(menu == '10') {		document.forms[0].action = 'VoidVendRequestAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'requestPage';	}	else if(menu == '11') {		document.forms[0].action = 'AckVendRequestAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'requestPage';				}	else if(menu == '12') {		document.forms[0].action = 'CustomerDispatchSearchAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'searchPage';				}	else if(menu == '13') {		document.forms[0].action = 'PremiseDispatchDetailAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'searchPage';				}	else if(menu == '25') {		document.forms[0].action = 'SourceDetailsAction';		document.forms[0].target = 'DisplayFrame';		document.forms[0].dispatch.value = 'processShow';	}		document.forms[0].submit();}</script><title>Menu Page</title><style type="text/css">body {	background: transparent none repeat scroll 0 0 !important;	font-weight: bold;	padding-left: 1em;	font-family: Georgia, "Times New Roman", Times, serif;	background-color: #6699ff;	color: Black;}</style></head><body><form method="post"><table border="1">	<tr>	 	<td background="black"><label onclick="submitPage('00')"style="cursor: hand"><bean:message key="app.home" /></label></td>		<logic:present name="USERFUNCTIONS">		<logic:iterate id="functionDetail" name="USERFUNCTIONS"  type="com.centrica.vms.model.FunctionDetails">         <bean:define id="code" name="functionDetail" property="functionCode" />         <bean:define id="description" name="functionDetail" property="functionDescription" />         <td background="black"><label onclick="submitPage('<bean:write name="code"/>')"			style="cursor: hand"><bean:write name="description"/></label></td>        </logic:iterate>        </logic:present>        <td background="black"><label onclick="submitPage('08')"style="cursor: hand"><bean:message key="app.changepwd" /></label></td>		<td><label onclick="logOff()" style="cursor: hand"><bean:message			key="app.logoff" /></label></td>	</tr></table><input type="hidden" name="dispatch"></form></body></html>