<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
  <display-name>VendManagementSystem</display-name>
  <welcome-file-list>
    <welcome-file>Welcome.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>log4j-init</servlet-name>
    <servlet-class>com.centrica.vms.servlet.Log4jInitializerServlet</servlet-class>
    <init-param>
      <param-name>log4j-init-file</param-name>
      <param-value>WEB-INF/classes/properties/log4j.config</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>SchedulerInitializer</servlet-name>
    <servlet-class>com.centrica.vms.servlet.SchedulerInitializer</servlet-class>
    <init-param>
      <param-name>QUZ_CONF</param-name>
      <param-value>/properties/Quartz.properties</param-value>
    </init-param>
    <init-param>
      <param-name>JOB_NAME</param-name>
      <param-value>vendcodejob</param-value>
    </init-param>
	<init-param>
		<param-name>QUZ_CACHE_CONF</param-name>
		<param-value>/properties/Quartz_Cache.properties</param-value>
	</init-param>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>VMS Initializer</servlet-name>
    <servlet-class>com.centrica.vms.servlet.VMSInitializerServlet</servlet-class>
    <init-param>
      <param-name>TIMEZONE</param-name>
      <param-value>UTC</param-value>
    </init-param>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet>
    <display-name>Apache-Axis Servlet</display-name>
    <servlet-name>AxisServlet</servlet-name>
    <servlet-class>org.apache.axis2.transport.http.AxisServlet</servlet-class>
    <load-on-startup>4</load-on-startup>
  </servlet>
  <servlet>
    <display-name>Apache-Axis AxisAdmin Servlet (Web Admin)</display-name>
    <servlet-name>AxisAdminServlet</servlet-name>
    <servlet-class>org.apache.axis2.webapp.AxisAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AxisServlet</servlet-name>
    <url-pattern>/servlet/AxisServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AxisServlet</servlet-name>
    <url-pattern>*.jws</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AxisServlet</servlet-name>
    <url-pattern>/services/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AxisAdminServlet</servlet-name>
    <url-pattern>/axis2-admin/*</url-pattern>
  </servlet-mapping>
  <mime-mapping>
    <extension>inc</extension>
    <mime-type>text/plain</mime-type>
  </mime-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>/axis2-web/index.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/axis2-web/Error/error404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/axis2-web/Error/error500.jsp</location>
  </error-page>
  <error-page>
	<exception-type>java.lang.Throwable</exception-type>
	<location>/error.jsp</location>
  </error-page>
  <servlet>
    <display-name>Axis Admin Servlet</display-name>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>org.apache.axis.transport.http.AdminServlet</servlet-class>
    <load-on-startup>100</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/servlet/AdminServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>VMSDispatchAction</servlet-name>
    <servlet-class>org.apache.struts.action.ActionServlet</servlet-class>
    <init-param>
      <param-name>config</param-name>
      <param-value>/WEB-INF/struts-config.xml</param-value>
    </init-param>
    <load-on-startup>5</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/MeterDispatchSearchAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/MeterDispatchDetailAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/CustomerDispatchSearchAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/CustomerDispatchDetailAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/PremiseDispatchSearchAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/PremiseDispatchDetailAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/VendHistorySearchAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/VendRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/VoidVendRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/AckVendRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/VMSHome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/PowerUserAdminAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/PowerUserAdminDetailAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/UserAdminAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/UserAdminDetailAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/ChangePwdAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/AdjustmentRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/UtilityRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/HEPhase2UtilityRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/HEPhase3UtilityRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/HECIMUtilityRequestAction</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>VMSDispatchAction</servlet-name>
    <url-pattern>/VendReportAction</url-pattern>
  </servlet-mapping>
	<servlet-mapping>
		<servlet-name>VMSDispatchAction</servlet-name>
		<url-pattern>/SourceDetailsAction</url-pattern>
	</servlet-mapping>
  <filter>
    <filter-name>VMSWebFilter</filter-name>
    <filter-class>com.centrica.vms.servlet.VMSWebActionFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>VMSWebFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>MenuPage</servlet-name>
    <jsp-file>/WEB-INF/jsp/MenuPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>MenuPage</servlet-name>
    <url-pattern>/MenuPage</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>HomePage</servlet-name>
    <jsp-file>/WEB-INF/jsp/MainPage.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomePage</servlet-name>
    <url-pattern>/HomePage</url-pattern>
  </servlet-mapping>
  <resource-ref>
    <description>Prepayment database for Vend Code Generation Applications</description>
    <res-ref-name>jdbc/PrepaymentDB</res-ref-name>
    <res-type>com.mchange.v2.c3p0.ComboPooledDataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <resource-env-ref>
    <description> Object factory for HeadendDetails instances.</description>
    <resource-env-ref-name>bean/VendCode</resource-env-ref-name>
    <resource-env-ref-type>com.centrica.vms.scheduler.external.model.VendServiceDetails</resource-env-ref-type>
  </resource-env-ref>
  <resource-env-ref>
    <description> Object factory for SAPPIDetails instances.</description>
    <resource-env-ref-name>bean/VendService</resource-env-ref-name>
    <resource-env-ref-type>com.centrica.vms.scheduler.external.model.VendServiceDetails</resource-env-ref-type>
  </resource-env-ref>
  <servlet>
    <servlet-name>VendMgmtSysTest</servlet-name>
    <servlet-class>com.centrica.unittest.VendMgmtSysServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VendMgmtSysTest</servlet-name>
    <url-pattern>/VendMgmtSysTest</url-pattern>
  </servlet-mapping>
    <servlet>
    <servlet-name>VMSSCNActivationTest</servlet-name>
    <servlet-class>com.centrica.unittest.VMSSCNActivationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VMSSCNActivationTest</servlet-name>
    <url-pattern>/VMSSCNActivationTest</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>VendTransactionDAOTest</servlet-name>
    <servlet-class>com.centrica.unittest.VendTransactionDAOServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VendTransactionDAOTest</servlet-name>
    <url-pattern>/VendTransactionDAOTest</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SchedulerTest</servlet-name>
    <servlet-class>com.centrica.unittest.SchedulerTestServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SchedulerTest</servlet-name>
    <url-pattern>/SchedulerTest</url-pattern>
  </servlet-mapping>
  <session-config>
     <session-timeout>30</session-timeout> 
   </session-config>
  
</web-app>
